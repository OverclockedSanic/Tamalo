{
	"compilerOptions": {
		"outDir": "dist",
		"module": "commonjs",
		"target": "es6",
		"lib": ["es6", "dom"],
		"sourceMap": true,
		"moduleResolution": "node",
		"rootDir": "src",
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"noImplicitAny": true,
		"strictNullChecks": true
	},
	"include" : [
		"src"
	],
	"exclude": [
		"node_modules",
		"build"
	]
}
